{
  "description": "Pipeline for parsing stats.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{DATA:ts}\t%{DATA:peer}\t%{DATA:mem}\t%{DATA:pkts_proc}\t%{DATA:bytes_recv}\t%{DATA:pkts_dropped}\t%{DATA:pkts_link}\t%{DATA:pkt_lag}\t%{DATA:events_proc}\t%{DATA:events_queued}\t%{DATA:active_tcp_conns}\t%{DATA:active_udp_conns}\t%{DATA:active_icmp_conns}\t%{DATA:tcp_conns}\t%{DATA:udp_conns}\t%{DATA:icmp_conns}\t%{DATA:timers}\t%{DATA:active_timers}\t%{DATA:files}\t%{DATA:active_files}\t%{DATA:dns_requests}\t%{DATA:active_dns_requests}\t%{DATA:reassem_tcp_size}\t%{DATA:reassem_file_size}\t%{DATA:reassem_frag_size}\t%{DATA:reassem_unknown_size}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.stats.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}