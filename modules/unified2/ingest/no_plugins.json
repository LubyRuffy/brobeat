{
  "description": "Pipeline for parsing unified2.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{DATA:ts}\t%{IP:orig_h}\t%{INT:orig_p}\t%{IP:resp_h}\t%{INT:resp_p}\t%{DATA:sensor_id}\t%{DATA:signature_id}\t%{DATA:signature}\t%{DATA:generator_id}\t%{DATA:generator}\t%{DATA:signature_revision}\t%{DATA:classification_id}\t%{DATA:classification}\t%{DATA:priority_id}\t%{DATA:event_id}\t%{DATA:packet}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.unified2.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}