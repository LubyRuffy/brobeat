{
  "description": "Pipeline for parsing netcontrol_catch_release.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{DATA:ts}\t%{DATA:rule_id}\t%{DATA:ip}\t%{DATA:action}\t%{DATA:block_interval}\t%{DATA:watch_interval}\t%{DATA:blocked_until}\t%{DATA:watched_until}\t%{DATA:num_blocked}\t%{DATA:location}\t%{DATA:message}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.netcontrol_catch_release.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}