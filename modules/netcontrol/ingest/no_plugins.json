{
  "description": "Pipeline for parsing netcontrol.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{DATA:ts}\t%{DATA:rule_id}\t%{DATA:category}\t%{DATA:cmd}\t%{DATA:state}\t%{DATA:action}\t%{DATA:target}\t%{DATA:entity_type}\t%{DATA:entity}\t%{DATA:mod}\t%{DATA:msg}\t%{DATA:priority}\t%{DATA:expire}\t%{DATA:location}\t%{DATA:plugin}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.netcontrol.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}