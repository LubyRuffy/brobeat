{
  "description": "Pipeline for parsing http.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{DATA:ts}\t%{NOTSPACE:uid}\t%{DATA:id.orig_h}\t%{DATA:id.orig_p}\t%{DATA:id.resp_h}\t%{DATA:id.resp_p}\t%{DATA:trans_depth}\t%{DATA:method}\t%{DATA:host}\t%{DATA:uri}\t%{DATA:referrer}\t%{DATA:version}\t%{DATA:user_agent}\t%{DATA:request_body_len}\t%{DATA:response_body_len}\t%{DATA:status_code}\t%{DATA:status_msg}\t%{DATA:info_code}\t%{DATA:info_msg}\t%{DATA:tags}\t%{DATA:username}\t%{DATA:password}\t%{DATA:capture_password}\t%{DATA:proxied}\t%{DATA:range_request}\t%{NOTSPACE:orig_fuids}\t%{DATA:orig_filenames}\t%{DATA:orig_mime_types}\t%{NOTSPACE:resp_fuids}\t%{DATA:resp_filenames}\t%{DATA:resp_mime_types}\t%{DATA:current_entity}\t%{DATA:orig_mime_depth}\t%{DATA:resp_mime_depth}\t%{DATA:client_header_names}\t%{DATA:server_header_names}\t%{DATA:omniture}\t%{DATA:flash_version}\t%{DATA:cookie_vars}\t%{DATA:uri_vars}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.http.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}