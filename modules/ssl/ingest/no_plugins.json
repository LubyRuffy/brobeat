{
  "description": "Pipeline for parsing ssl.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{DATA:ts}\t%{NOTSPACE:uid}\t%{DATA:id.orig_h}\t%{DATA:id.orig_p}\t%{DATA:id.resp_h}\t%{DATA:id.resp_p}\t%{DATA:version_num}\t%{DATA:version}\t%{DATA:cipher}\t%{DATA:curve}\t%{DATA:server_name}\t%{DATA:session_id}\t%{DATA:resumed}\t%{DATA:client_ticket_empty_session_seen}\t%{DATA:client_key_exchange_seen}\t%{DATA:server_appdata}\t%{DATA:client_appdata}\t%{DATA:last_alert}\t%{DATA:next_protocol}\t%{DATA:analyzer_id}\t%{DATA:established}\t%{DATA:logged}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.ssl.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}