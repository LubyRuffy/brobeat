{
  "description": "Pipeline for parsing smb_cmd.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{NUMBER:ts}\t%{NOTSPACE:uid}\t%{IP:id.orig_h}\t%{INT:id.orig_p}\t%{IP:id.resp_h}\t%{INT:id.resp_p}\t%{DATA:command}\t%{DATA:sub_command}\t%{DATA:argument}\t%{DATA:status}\t%{DATA:rtt}\t%{DATA:version}\t%{DATA:username}\t%{DATA:tree}\t%{DATA:tree_service}\t%{NUMBER:referenced_file.ts}\t%{NOTSPACE:referenced_file.uid}\t%{NOTSPACE:referenced_file.id}\t%{NOTSPACE:referenced_file.fuid}\t%{DATA:referenced_file.action}\t%{DATA:referenced_file.path}\t%{DATA:referenced_file.name}\t%{INT:referenced_file.size}\t%{DATA:referenced_file.prev_name}\t%{NUMBER:referenced_file.times.modified}\t%{NUMBER:referenced_file.times.accessed}\t%{NUMBER:referenced_file.times.created}\t%{NUMBER:referenced_file.times.changed}\t%{INT:referenced_file.fid}\t%{NOTSPACE:referenced_file.uuid}\t%{NUMBER:referenced_tree.ts}\t%{NOTSPACE:referenced_tree.uid}\t%{NOTSPACE:referenced_tree.id}\t%{DATA:referenced_tree.path}\t%{DATA:referenced_tree.service}\t%{DATA:referenced_tree.native_file_system}\t%{DATA:referenced_tree.share_type}\t%{DATA:smb1_offered_dialects}\t%{DATA:smb2_offered_dialects}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.smb_cmd.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}