{
  "description": "Pipeline for parsing rdp.log. Requires no plugins",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{DATA:ts}\t%{NOTSPACE:uid}\t%{DATA:id.orig_h}\t%{DATA:id.orig_p}\t%{DATA:id.resp_h}\t%{DATA:id.resp_p}\t%{DATA:cookie}\t%{DATA:result}\t%{DATA:security_protocol}\t%{DATA:keyboard_layout}\t%{DATA:client_build}\t%{DATA:client_name}\t%{DATA:client_dig_product_id}\t%{DATA:desktop_width}\t%{DATA:desktop_height}\t%{DATA:requested_color_depth}\t%{DATA:cert_type}\t%{DATA:cert_count}\t%{DATA:cert_permanent}\t%{DATA:encryption_level}\t%{DATA:encryption_method}\t%{DATA:analyzer_id}\t%{DATA:done}\t%{DATA:ssl}"
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "rename": {
      "field": "@timestamp",
      "target_field": "read_timestamp"
    }
  }, {
    "date": {
      "field": "bro.rdp.ts",
      "target_field": "@timestamp",
      "formats": ["dd/MMM/YYYY:H:m:s Z"]
    }
  }, {
    "remove": {
      "field": "nginx.access.time"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}